<Window x:Class="WpfApp1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Window.CommandBindings>
        <CommandBinding Command="Open" Executed="CommandBinding_Executed_Open"></CommandBinding>
    </Window.CommandBindings>
    <Grid>
        <Button Content="Button" Command="New" HorizontalAlignment="Left" Margin="371,206,0,0" VerticalAlignment="Top"/>
        <Button Content="Button" Command="Open" HorizontalAlignment="Left" Margin="371,262,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
